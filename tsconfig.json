{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
		"paths": {
			"@/components/*": ["components/*"],
			"@/utils/*": ["utils/*"],
			"@/contexts/*": ["contexts/*"],
			"@/hooks/*": ["hooks/*"],
			"@/scripts/*": ["scripts/*"]
		}
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "postcss.config.js", "postcss.config.js", "next.config.js", "tailwind.config.js"  ],
  "exclude": [
    "node_modules"
  ]
}